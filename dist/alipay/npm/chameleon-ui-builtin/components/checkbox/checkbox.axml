<view class="cml-checkbox  cml-base cml-view cml-7497d5db" style="{{computedCstyle}}">
  <view class="cml-checkbox-wrap  cml-base cml-view cml-7497d5db" onTap="_cmlEventProxy" data-eventtap="{{['changeCheck']}}">
    <view class="cml-checkbox-check  cml-base cml-view cml-7497d5db" style="{{checkStyle}}">
      <view class="{{ checkClass }}  cml-base cml-view cml-7497d5db">
        <image class="inner-icon  cml-base cml-image cml-7497d5db" src="{{ checkImg }}" />
      </view>
    </view>
    <text class="cml-checkbox-label  cml-base cml-text cml-7497d5db">{{label}}</text>
  </view>
</view>