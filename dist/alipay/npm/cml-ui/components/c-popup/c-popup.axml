<view a:if="{{show}}" class="c-popup  cml-base cml-view cml-cd5eb5da" style="{{containerStyle}}">
  <view class="{{maskClass}}  cml-base cml-view cml-cd5eb5da" onTap="_cmlEventProxy" data-eventtap="{{['closeevent']}}"></view>
  
  <view class="c-popup-content  cml-base cml-view cml-cd5eb5da" style="{{contentStyle}}" catchTap="_cmlEventProxy" data-eventtap="{{['contentTap']}}">
    <slot class=" cml-base cml-slot cml-cd5eb5da"></slot>
  </view>
</view>