<picker-view indicator-style="height: 72rpx;" style="height: {{height}}rpx;" value="{{value}}" onChange="bindChange" class=" cml-base cml-origin-picker-view cml-4647f921">
  <picker-view-column class=" cml-base cml-origin-picker-view-column cml-4647f921">
    <view a:for="{{list}}" style="line-height:72rpx;align-items:center" class=" cml-base cml-view cml-4647f921">
      <text style="{{computedItemStyle}}" class=" cml-base cml-text cml-4647f921">{{item}}</text>
    </view>
  </picker-view-column>
</picker-view>