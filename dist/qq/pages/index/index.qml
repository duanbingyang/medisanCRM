<cml-buildin-page title="chameleon" class=" cml-base cml-page">
    <cml-buildin-scroller height="{{-1}}" class=" cml-base cml-scroller">
      <view class="scroller-wrap  cml-base cml-view" animation="{{animationData[0]}}" bindtransitionend="_cmlInline" data-eventtransitionend="{{['_animationCb','animationData[0]','$event']}}" bindtap="_cmlEventProxy" data-eventtap="{{['handleClick']}}">
        <demo-com title="{{title}}" image-src="{{chameleonSrc}}" class=" cml-view cml-demo-com"></demo-com>
      </view>
    </cml-buildin-scroller>
</cml-buildin-page>